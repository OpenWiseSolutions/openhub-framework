{
  "name": "admin-console",
  "version": "0.0.3",
  "license": "Apache-2.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:mock": "better-npm-run dev:mock",
    "mock": "better-npm-run mock",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm test"
  },
  "betterScripts": {
    "mock": {
      "command": "node ./mock-server/index.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "dev:mock": {
      "command": "parallelshell \"npm run mock\" \"npm run dev\"",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "compression": "^1.6.2",
    "debug": "^2.2.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "moment": "^2.17.1",
    "object-path": "^0.11.3",
    "radium": "^0.18.1",
    "react": "^15.0.0",
    "react-animations": "^0.1.0",
    "react-dom": "^15.0.0",
    "react-icons": "^2.2.1",
    "react-modal": "^1.6.5",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "recharts": "^0.20.8",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.4",
    "tinycolor2": "^1.4.1",
    "url-loader": "^0.5.6",
    "valid-react-form": "^0.2.1",
    "webpack": "^1.12.14",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "0.5.2",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "enzyme": "^2.0.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.1",
    "express-mock-server": "^1.1.1",
    "husky": "^0.13.2",
